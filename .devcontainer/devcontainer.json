{
	"name": "Ubuntu Flutter and Android SDK with Emulator",
	"build": {
        "dockerfile": "./Dockerfile",
        "context": "."
    },
	"features": {
		"ghcr.io/devcontainers/features/desktop-lite:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Dart-Code.flutter"
			]
		}
	},
	"forwardPorts": [
		6080
	],
	"postCreateCommand": "/usr/local/finish.sh"
}