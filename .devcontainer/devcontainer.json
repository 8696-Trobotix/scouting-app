{
	"name": "Ubuntu Flutter and Android SDK with Emulator",
	"build": {
		"dockerfile": "./Dockerfile",
		"context": "."
	},
	"features": {
		"ghcr.io/devcontainers/features/java:1": {
			"installGradle": true,
			"version": "latest",
			"jdkDistro": "ms",
			"gradleVersion": "latest",
			"mavenVersion": "latest",
			"antVersion": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Dart-Code.flutter"
			]
		}
	},
	"postCreateCommand": "/usr/local/finish.sh"
}